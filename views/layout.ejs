<% 
var mainActive ="";
var galleryActive="";
var mymodelActive="";
var profileActive="";
var helpActive ="";
var loginActive  ="";
switch (title) {
    case "Main":
		 mainActive = 'active';  break;
    case "Gallery":
        galleryActive = 'active'; break;
	case "My Models":
        mymodelActive = 'active'; break;
    case "Profile":
        profileActive = 'active';  break;
    case "Help":
        helpActive = 'active';  break;
	case "Login":
		loginActive = 'active'; break;
};

 %>								
                                <li class="<%= mainActive %>">
                                <a href="index">
                                Main </a>
                                </li>
                                <li class="<%= galleryActive %>">
                                <a href="gallery">
                                Gallery </a>
                                </li>
                                <li class="<%= mymodelActive %>">
                                <a href="mymodels" >
                                My Models </a>
                                </li>
                                <li class="<%= profileActive %>">
                                <a href="profile">
                                Profile </a>
                                </li>
                                <li class="<%= helpActive %>">
                                <a href="help">
                                Help </a>
                                </li>
                        </ul>
                        <ul class="nav navbar-nav navbar-right">
							<% if (username != null && username != "" ) { %>						
                                <li>
                                <a href="profile">Hello <%= username %></a>
                                </li>
                                <li>
                                <a href="logout">
                                Log Out </a>
                                </li>
							<% } else { %>
							<li class="<%= loginActive %>">
                                <a href="login">
                                Log In</a>
                                </li>
							<% } %>
                        </ul>
