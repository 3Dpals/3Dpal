<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>3Dpal</title>
	<subtitle type="text">
    3Dpal Website - Blog, Documentation, Resources
	</subtitle>
	
 <link href="http://3Dpals.github.io/3Dpal/atom.xml" rel="self"/>
 <link href="http://3Dpals.github.io/3Dpal/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://3Dpals.github.io/3Dpal/</id>
 <author>
   <name>3Dpals - Johannes Baldinger & Benjamin Bill Planche</name>
   <uri>http://github.com/3Dpals/</uri>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://3Dpals.github.io/3Dpal{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://3Dpals.github.io/3Dpal{{ post.id }}</id>
   <content type="html">{{ post.content}}</content>
 </entry>
 {% endfor %}
 
</feed>
